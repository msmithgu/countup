html, body
  margin 0
  border 0
  padding 0
body
  background #222
  color #fff
  font-family 80px Helvetica, Arial, sans-serif
  text-align center
  padding 30px 40px
#countup
  #countup-display
    font-size 80px
  #countup-average
    font-size 18px
    margin 30px
    color #ccc
  ul#countup-logs
    padding 0
    margin 30px
    font-size 16px
    li
      padding 0
      margin 0
      list-style none
  input
    background #ddd
    border 3px solid black
    color #000
    font-size 40px
